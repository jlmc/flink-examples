<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">

    <Appenders>
        <Console name="ConsoleJson" target="SYSTEM_OUT">
            <JsonLayout compact="true" eventEol="true"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="info">
            <AppenderRef ref="ConsoleJson"/>
        </Root>
    </Loggers>

</Configuration>
